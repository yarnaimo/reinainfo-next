  "main": "dist/server/index.js",
  "scripts": {
    "build": "tsc",
    "debug": "node -r ts-node/register --inspect-brk",
    "deploy": "ts-node server/scripts/migrate.ts && firebase deploy",
    "lint": "eslint --ignore-path .gitignore '**/*.{js,jsx,ts,tsx}'",
    "logs": "firebase functions:log",
    "next:build": "NODE_ENV=production next build",
    "next:debug": "node -r ts-node/register --inspect server/next-dev",
    "next:dev": "TS_NODE_PROJECT=tsconfig.server.json ts-node server/next-dev",
    "np": "np --yolo --no-publish",
    "prepare": "./fix-bluespark.sh",
    "serve": "firebase emulators:start --only firestore",
    "serve:functions": "tsc && firebase emulators:start --only firestore,functions",
    "shell": "tsc && firebase functions:shell",
    "test": "NODE_ENV=test jest",
    "ts-node": "TS_NODE_PROJECT=tsconfig.server.json ts-node"
  },
